Algoritmo sin_titulo
	Dimension a[6,4]
	Para i<-1 hasta 6 Hacer
	    Para j<-1 hasta 4 hacer 
			Leer a[i,j]
		FinPara
	FinPara
	Escribir a[2,4]
	
	suma<-0
	
	Para i<-1 hasta 6 hacer
		Escribir a[i,3]
		suma<-suma+a[i,j]
	FinPara
	Escribir suma 
	Para j<-1 hasta 4 Hacer
		Escribir a[5,j]
    Fin para
		
FinAlgoritmo
